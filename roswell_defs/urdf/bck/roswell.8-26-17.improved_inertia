        <box size="0.0254 0.0254 1.4224"/>
      </geometry>
      <material name="Grey"/>
    </visual>
    <inertial>
      <!-- 56" of 8020 is 1.2 kg or 2.5 lbs -->
      <mass value="1.2" /> 
      <origin rpy="0 0 0" xyz="-0.0127 0.0127 0.7112" />
      <!-- inertia derivation (see wikipedia)
      Solid cuboid of width w, height h, depth d, and mass m
      <box size="0.0254 0.0254 1.4224"/>
           xx= m(d^2+h^2)/12 = 1.2*(.0254*.0254+1.4224*1.4224)/12 = .2023867
           yy= m(w^2+d^2)/12 = 1.2*(.0254*.0254+.0254*.0254)/12 = .000129
           zz= m(w^2+h^2) == xx = .2023867
           original:
           <inertia ixx="0.2023867" ixy="-0.0" ixz="0.0" iyx="-0.0" iyy="0.000129" iyz="-0.0" izx="0.0" izy="-0.0" izz="0.2023867" />
      -->
      <inertia ixx="0.2023867" ixy="-0.0" ixz="0.0" iyx="-0.0" iyy="0.000129" iyz="-0.0" izx="0.0" izy="-0.0" izz="0.2023867" />
      <collision>
        <origin rpy="0 0 0 " xyz="0 0 -0.0762"/>
        <geometry>
          <box size="0.0254 0.0254 1.4224"/>
        </geometry>
      </collision>
    </inertial>
  </link>
