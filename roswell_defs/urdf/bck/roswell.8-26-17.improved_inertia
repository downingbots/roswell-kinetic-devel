  <link name="torso_actuator_link">
    <!--
      https://www.firgelliauto.com/products/mini-track-actuator
      35 lb dynamic force, 2"/s, 4lbs weight
      use h = 0.635, w=d=.04064
    -->
    <inertial>
      <!-- more accurate mass and intertia:
        <mass value="1.814" />
        xx = m(h^2+d^2)/ 12  yy= m(w^2+d^2)/12, zzz = m(w^2+h^2)/12
        xx == zz = 1.814*(.635*.635+.04064*.04064) = .734446
        yy == 1.814*(.04064*.04064+.04064*.04064) = .005992
        <inertia ixx="0.734446" ixy="-0.0001" ixz="-0.0" iyx="-0.0001" iyy="0.005992" iyz="0.0" izx="-0.0" izy="0.0" izz="0.734446" />
      -->
      <origin rpy="0 0 0" xyz="0.015 0.0127 0.3251" />
      <mass value="1.814" />
      <inertia ixx="0.734446" ixy="-0.0001" ixz="-0.0" iyx="-0.0001" iyy="0.005992" iyz="0.0" izx="-0.0" izy="0.0" izz="0.734446" />
    </inertial>
