  </joint>
<macro name="cylinder_inertia" params="m r h">
          <inertia  ixx="${m*(3*r*r+h*h)/12}" ixy = "0" ixz = "0"
                    iyy="${m*(3*r*r+h*h)/12}" iyz = "0"
                    izz="${m*r*r/2}" /> 
</macro>
  <link name="base_l_wheel_link">
    <inertial>
      <!--
       <cylinder_inertia  m=".3531" r="0.06191" h="0.02031" />
       .3531*( 3*.06*.06 + .02*.02) / 12 = .00033
       .3531*.06*.06/2 = .00063558
      -->
      <origin rpy="0 0 0" xyz="0 0 0" />
      <mass value="0.3542" />
      <inertia ixx="0.00033" ixy="0.0" ixz="0.0"
               iyy="0.00033" iyz="0.0"
               izz="0.00063558" />
    </inertial>
    <visual>
