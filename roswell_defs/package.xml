<package>
  <name>roswell_defs</name>
  <version>0.2.0</version>
  <description>
    Maxwell is a custom mobile manipulator, this package contains his configuration and launch files. 
  </description>
  <author>Michael Ferguson</author>
  <maintainer email="mike@vanadiumlabs.com">Michael Ferguson</maintainer>
  <license>BSD</license>
  <url>http://ros.org/wiki/roswell_defs</url>
  
  <buildtool_depend>catkin</buildtool_depend>

  <!-- definitions -->
  <run_depend>urdf</run_depend>
  <run_depend>xacro</run_depend>

  <!-- hardware -->
  <run_depend>arbotix_controllers</run_depend>
  <run_depend>diagnostic_aggregator</run_depend>
  <run_depend>etherbotix_python</run_depend>
  <run_depend>openni2_launch</run_depend>
  <run_depend>robot_state_publisher</run_depend>
  <run_depend>urg_node</run_depend>

  <!-- teleop -->
  <run_depend>joy</run_depend>
  <run_depend>ps3joy</run_depend>

</package>
