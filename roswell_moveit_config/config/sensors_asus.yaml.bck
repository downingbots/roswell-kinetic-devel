sensors:
  - sensor_plugin: occupancy_map_monitor/PointCloudOctomapUpdater
    point_cloud_topic: /head_camera/depth_registered/points
    filtered_cloud_topic: /head_camera/filtered/points
    max_range: 3.0
    frame_subsample: 1
    point_subsample: 1
    padding_offset: 0.03
    padding_scale: 1.0
